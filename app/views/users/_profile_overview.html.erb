<div class="tab-pane fade show active profile-overview" id="profile-overview">
  <h5 class="card-title">Profile Details</h5>
  <div class="row">
    <div class="col-lg-3 col-md-4 label">First Name</div>
    <div class="col-lg-9 col-md-8"> <%= current_user.first_name %></div>
  </div>

  <div class="row">
    <div class="col-lg-3 col-md-4 label">Last Name</div>
    <div class="col-lg-9 col-md-8" > <%= current_user.last_name %></div>
  </div>

  <div class="row">
    <div class="col-lg-3 col-md-4 label">Country</div>
    <div class="col-lg-9 col-md-8"><%= current_user.country %></div>
  </div>

  <div class="row">
    <div class="col-lg-3 col-md-4 label">State</div>
    <div class="col-lg-9 col-md-8"><%= current_user.state %></div>
  </div>

  <div class="row">
    <div class="col-lg-3 col-md-4 label">Phone</div>
    <div class="col-lg-9 col-md-8"><%= current_user.phone %></div>
  </div>
</div>
