<% if @error %>
  $("#UpdatePasswordError").html("<%= j render 'shared/errors', resource: @user %>")
<% else %>
  $("#flash").html("<%= j render 'shared/flash_messages'%>")
  setTimeout(function () {
    $("#ChangePasswordForm")[0].classList.remove("was-validated")
    $("#resetChangePasswordForm").click();
  }, 1000);
<% end %>


